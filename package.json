{
    "name": "@sanguogege/calendar",
    "version": "1.2.2",
    "keywords": [
        "日历",
        "农历",
        "阳历",
        "calendar"
    ],
    "description": "ts版本的日历生成器 LTS",
    "author": "sanguogege",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "repository": "https://github.com/sanguogege/calendar",
    "files": [
        "dist"
    ],
    "scripts": {
        "change": "bun changeset",
        "version": "bun changeset version",
        "status": "bun changeset status",
        "change:patch": "bun changeset --empty --type patch",
        "change:minor": "bun changeset --empty --type minor",
        "change:major": "bun changeset --empty --type major",
        "publish": "bun publish --all"
    },
    "devDependencies": {
        "@changesets/cli": "^2.29.8",
        "@vitejs/plugin-legacy": "^7.2.1",
        "rollup-plugin-dts": "^6.3.0",
        "terser": "^5.44.1",
        "typescript": "^5.9.3",
        "vite": "^7.3.0",
        "vite-plugin-build": "^0.10.0",
        "vite-plugin-dts": "^4.5.4",
        "vitest": "^4.0.16"
    },
    "workspaces": [
        "packages/*"
    ]
}